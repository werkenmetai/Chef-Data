{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.werkenmetai/exact-online-mcp",
  "title": "Praat met je Boekhouding",
  "description": "Query Exact Online accounting data via AI. 44+ tools for invoices, cashflow, reports.",
  "repository": {
    "url": "https://github.com/werkenmetai/Exact-online-MCP",
    "source": "github"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@werkenmetai/exact-online-mcp",
      "version": "1.0.0",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "EXACT_MCP_URL",
          "description": "Remote MCP server URL (default: https://api.praatmetjeboekhouding.nl/mcp)",
          "isRequired": false,
          "format": "string",
          "isSecret": false
        }
      ]
    }
  ]
}
